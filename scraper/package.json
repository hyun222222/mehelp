{
    "name": "legal-scraper",
    "version": "1.0.0",
    "description": "Legal forms and cases scraper for helplaw24.go.kr and pro-se.scourt.go.kr",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/index.ts",
        "crawl:forms": "ts-node src/index.ts --target forms",
        "crawl:cases": "ts-node src/index.ts --target cases",
        "crawl:prose": "ts-node src/index.ts --target prose",
